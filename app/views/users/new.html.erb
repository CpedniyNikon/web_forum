<h2> Join and ask </h2>
<%= form_with model: @user do |form|%>
  <p> <%= form.label :nickname, 'Your nickname' %> </p>
  <p> <%= form.text_field :nickname%> </p>
  <p> <%= form.label :password, 'Your password' %> </p>
  <p> <%= form.password_field :password%> </p>
  <p> <%= form.label :password_confirmation, 'Your password' %> </p>
  <p> <%= form.password_field :password_confirmation%> </p>
  <p> <%= form.submit 'Sign up'%></p>
<%end%>