<h2> Sign in </h2>
<p> Enter Your nickname and password to join </p>
<%= form_with scope: :session, url: session_path do |form|%>
  <p> <%= form.label :nickname%> </p>
  <p> <%= form.text_field :nickname%> </p>
  <p> <%= form.label :password%> </p>
  <p> <%= form.password_field :password%> </p>
  <p> <%= form.submit 'Sign in'%></p>
<%end%>
