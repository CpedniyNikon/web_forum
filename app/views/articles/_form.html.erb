<%= form_with model: article do |form| %>
  <p><%= form.label :user_id, 'user id'%></p>
  <p><%= form.text_field :user_id, rows: 5%></p>
  <p><%= form.label :title, 'Article title' %></p>
  <p><%= form.text_area :title, rows: 5 %></p>
  <p><%= form.label :body, 'Article body' %></p>
  <p><%= form.text_field :body %></p>
  <p><%= form.submit "#{action}" %></p>
<%end%>